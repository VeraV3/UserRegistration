<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
 


  <display-name>Web Application</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <servlet-name>ServletSubmit</servlet-name>
    <servlet-class>servers.ServletSubmit</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletSubmit</servlet-name>
    <url-pattern>/ServletSubmit</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <servlet-name>SendXml</servlet-name>
    <servlet-class>servers.SendXml</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SendXml</servlet-name>
    <url-pattern>/SendXml</url-pattern>
  </servlet-mapping>
 

</web-app>
